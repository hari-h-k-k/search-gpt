{
  "manifest_version": 3,
  "name": "Search GPT",
  "version": "1.0",
  "description": "Search in Chat GPT",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/SGPT.png",
    "48": "icons/SGPT.png",
    "128": "icons/SGPT.png"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
